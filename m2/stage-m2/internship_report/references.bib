	month = jan,
	year = {2020},
	note = {Publisher: Society for Industrial and Applied Mathematics},
	keywords = {community detection, 05C85, 62F15, 62F35, 62M15, network 
	data analysis, random graphs, spectral algorithms, spectral embeddings, 
	stochastic block models},
	pages = {132--157},
	file = 
	{10.1137@19M1257135.pdf:/Users/leobianco/Zotero/storage/2EI8UGWD/10.1137@19M1257135.pdf:application/pdf;Submitted
	 Version:/Users/leobianco/Zotero/storage/LIGR25LY/Abbe et al. - 2020 - 
